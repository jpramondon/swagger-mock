{
  "openapi": "3.0.0",
  "info": {
    "title": "default-sample-api",
    "description": "A default sample API provided as an example",
    "version": "0.0.1",
    "contact": {}
  },
  "servers": [],
  "components": {
    "schemas": {
      "Toast": {
        "type": "object",
        "properties": {
          "key": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        }
      }
    }
  },
  "paths": {
    "/api/toast/{id}": {
      "get": {
        "operationId": "VendorController_getVendor",
        "parameters": [
          {
            "name": "id",
            "required": true,
            "in": "path",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "The toast matching the provided ID",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Toast"
                }
              }
            }
          },
          "404": {
            "description": "Whenever no toast could be matched with the provided ID"
          }
        }
      }
    }
  }
}